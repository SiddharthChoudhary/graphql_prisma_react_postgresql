// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users{
  id       Int     @id @default(autoincrement())
  name     String?
  username String?
  email    String?
  password String?
  profile_picture  String
}
